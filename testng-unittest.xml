<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "testng-1.0.dtd" >
<suite name="units" verbose="1">
	<listeners>
	      <listener class-name="org.uncommons.reportng.HTMLReporter"/>
	      <listener class-name="org.uncommons.reportng.JUnitXMLReporter"/>
	</listeners>
	<test name="unitTests">
		<groups>
			<run>
				<include name="unit" />
			</run>
		</groups>
	
		<packages>
			<package name="test.java.com.github.vamem9z.dci.accounts.contexts" />
			<package name="test.java.com.github.vamem9z.dci.accounts.roles" />
			<package name="test.java.com.github.vamem9z.dci.accounts.rules" />
			<package name="test.java.com.github.vamem9z.dci.contexts" />
		</packages>
	</test>
</suite>

